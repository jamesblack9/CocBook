@{
    ViewBag.Title = "Manage Order";
    Layout = "~/Views/Shared/_Admin.cshtml";
}
<div class="container">
    <h2>
        Manage Order</h2>
    <div id='left-menu-container' class="span3">
        <ul id='left-menu'>
            <li>
                <div class="menu-block">
                    <div class="subhead">
                    </div>
                    <div class="block-body">
                        <ul class="nav nav-list">
                            <li><a href="#" onclick="javascript:GetListOrderView()">List Order</a></li>
                        </ul>
                    </div>
                </div>
            </li>
        </ul>
    </div>
    <div id='book-list'>
        <div id="new-book" class='book-div'>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        GetListOrderView();
    });
    function GetListOrderView() {
        $('#book-list').load("@Url.Action("ListOrder", "Admin")");
    }
</script>
@if (false)
{  
    <script src="/Scripts/jquery-1.5.1-vsdoc.js" type="text/javascript">

    </script> 
}
